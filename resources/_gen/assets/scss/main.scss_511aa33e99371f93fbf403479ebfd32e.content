*{box-sizing:border-box;margin:0;padding:0}body{font-family:Arial,sans-serif;background-color:#fff;color:#000}a{color:#007bff;text-decoration:none}a:hover{text-decoration:underline}body{background:#fff}#orbit{height:100vh;overflow:scroll;width:100vw;position:fixed;top:0;left:0;padding-left:20px;padding-top:20px;padding-right:20px;padding-bottom:50px;font-size:10vh;font-weight:700;font-family:sans-serif;color:#000;line-height:95%;margin:0;border:0 solid;word-wrap:normal}.no-scroll{overflow:hidden;position:absolute}.drawer-brand{font-size:15vh;font-weight:700;font-family:sans-serif}#about{display:none;z-index:1000;background:#fff}.history-container{max-height:50%;overflow:scroll}#videoElement{height:100vh;width:100vw;background-color:#000;opacity:.5;margin:0;padding:0;background-size:auto;display:none}#toggle-video-button{display:none}.video-text{text-shadow:-1px -1px 0 #fff,1px -1px 0 #fff,-1px 1px 0 #fff,1px 1px 0 #fff;background:0 0}.normal{filter:invert(0%);background:#fff}.inverted{filter:invert(100%);background:#000}@media(min-width:769px){.history-modal .modal-content{max-width:600px;margin:10% auto;border-radius:8px}}@media(max-width:768px){.history-modal .modal-content{width:100%;height:100%;margin:0;border-radius:0}}.history-content{max-height:70vh;overflow-y:auto}@media(max-width:768px){.history-content{max-height:calc(100vh - 60px)}}.history-item{padding:15px;border-bottom:1px solid #e9ecef;cursor:pointer;transition:background-color .2s ease}.history-item:last-child{border-bottom:none}.history-item:hover{background-color:#e6f2ff}.history-empty{text-align:center;padding:20px;color:#6c757d}.history-content::-webkit-scrollbar{width:8px}.history-content::-webkit-scrollbar-track{background:#fff}.history-content::-webkit-scrollbar-thumb{background-color:#6c757d;border-radius:4px;border:2px solid #fff}.modal{display:none;position:fixed;z-index:2000;left:0;top:0;width:100%;height:100%;overflow:auto;background-color:rgba(0,0,0,.4)}.modal .modal-content{background-color:#fefefe;padding:20px;border:1px solid #888;width:80%}@media(max-width:768px){.modal .modal-content{width:100%;height:100%;border:none}}@media(max-width:768px){.modal{background-color:#fff}}.modal .modal-content{background-color:#fefefe;padding:20px;border:1px solid #888;width:80%}@media(max-width:768px){.modal .modal-content{width:100%;height:100%;border:none}}.modal .close{color:#aaa;float:right;font-size:28px;font-weight:700;cursor:pointer}.modal .close:hover,.modal .close:focus{color:#000;text-decoration:none;cursor:pointer}.btn{display:inline-block;padding:10px 20px;border:none;border-radius:5px;cursor:pointer;font-size:16px;text-align:center;text-decoration:none;transition:background-color .3s ease}.btn-primary{background-color:#007bff;color:#fff}.btn-primary:hover{background-color:#0062cc}.btn-sm{padding:5px 10px;font-size:14px}.controls{position:fixed;bottom:20px;right:20px;display:flex;gap:10px;z-index:1000}.control-btn{background-color:rgba(255,255,255,.8);border:none;border-radius:50%;width:50px;height:50px;font-size:24px;display:flex;align-items:center;justify-content:center;cursor:pointer;transition:background-color .3s ease,transform .2s ease}.control-btn:hover{background-color:#fff;transform:scale(1.05)}.control-btn:active{transform:scale(.95)}.control-btn .icon{line-height:1}#toggleVideoBtn{background-color:rgba(255,0,0,.8);color:#fff}#toggleVideoBtn:hover{background-color:red}#toggleVideoBtn.active{background-color:rgba(0,255,0,.8)}#toggleVideoBtn.active:hover{background-color:lime}@media(max-width:768px){.controls{display:none}}.text-center{text-align:center}.mt-1{margin-top:.25rem}.mt-2{margin-top:.5rem}.mt-3{margin-top:1rem}.mt-4{margin-top:1.5rem}.mt-5{margin-top:3rem}.mb-1{margin-bottom:.25rem}.mb-2{margin-bottom:.5rem}.mb-3{margin-bottom:1rem}.mb-4{margin-bottom:1.5rem}.mb-5{margin-bottom:3rem}.modal.mobile-view{background-color:#fff}.modal.mobile-view .modal-content{width:100%;height:100%;margin:0;border-radius:0}