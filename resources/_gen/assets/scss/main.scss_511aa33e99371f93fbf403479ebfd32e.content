*{box-sizing:border-box;margin:0;padding:0}body{font-family:Arial,sans-serif;background-color:#fff;color:#000}a{color:#007bff;text-decoration:none}a:hover{text-decoration:underline}body{background:#fff}#orbit{height:100vh;overflow:scroll;width:100vw;position:fixed;top:0;left:0;padding-left:20px;padding-top:20px;padding-right:20px;padding-bottom:50px;font-size:10vh;font-weight:700;font-family:sans-serif;color:#000;line-height:95%;margin:0;border:0 solid;word-wrap:normal}.no-scroll{overflow:hidden;position:absolute}.drawer-brand{font-size:15vh;font-weight:700;font-family:sans-serif}#about{display:none;z-index:1000;background:#fff}.history-container{max-height:50%;overflow:scroll}#videoElement{height:100vh;width:100vw;background-color:#000;opacity:.5;margin:0;padding:0;background-size:auto;display:none}#toggle-video-button{display:none}.video-text{text-shadow:-1px -1px 0 #fff,1px -1px 0 #fff,-1px 1px 0 #fff,1px 1px 0 #fff;background:0 0}.normal{filter:invert(0%);background:#fff}.inverted{filter:invert(100%);background:#000}.drawer{position:fixed;top:0;left:-250px;width:250px;height:100%;background-color:#fff;transition:left .3s ease-in-out;z-index:1000;box-shadow:2px 0 5px rgba(0,0,0,.1)}.drawer.open{left:0}.drawer-content{padding:20px}.drawer-header{font-size:24px;font-weight:700;margin-bottom:20px}.drawer-menu{list-style-type:none;padding:0}.drawer-menu li{margin-bottom:10px}.drawer-menu a{color:#000}.modal{display:none;position:fixed;z-index:2000;left:0;top:0;width:100%;height:100%;background-color:rgba(0,0,0,.5)}.modal-content{background-color:#fff;margin:15% auto;padding:20px;border-radius:5px;width:80%;max-width:500px}.modal .close{color:#6c757d;float:right;font-size:28px;font-weight:700;cursor:pointer}.modal .close:hover{color:#000}.btn{display:inline-block;padding:10px 20px;border:none;border-radius:5px;cursor:pointer;font-size:16px;text-align:center;text-decoration:none;transition:background-color .3s ease}.btn-primary{background-color:#007bff;color:#fff}.btn-primary:hover{background-color:#0062cc}.btn-sm{padding:5px 10px;font-size:14px}.text-center{text-align:center}.mt-1{margin-top:.25rem}.mt-2{margin-top:.5rem}.mt-3{margin-top:1rem}.mt-4{margin-top:1.5rem}.mt-5{margin-top:3rem}.mb-1{margin-bottom:.25rem}.mb-2{margin-bottom:.5rem}.mb-3{margin-bottom:1rem}.mb-4{margin-bottom:1.5rem}.mb-5{margin-bottom:3rem}